<h1>Listing Company Configuration Tables</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Value</th>
      <th>Comp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @company_configuration_tables.each do |company_configuration_table| %>
      <tr>
        <td><%= company_configuration_table.key %></td>
        <td><%= company_configuration_table.value %></td>
        <td><%= company_configuration_table.comp_id %></td>
        <td><%= link_to 'Show', company_configuration_table %></td>
        <td><%= link_to 'Edit', edit_company_configuration_table_path(company_configuration_table) %></td>
        <td><%= link_to 'Destroy', company_configuration_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

